
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<form action="Index" method="post">
    <fieldset>
        <legend>Login</legend>
        <ul>
            <li>
                <label for="user-id">아이디</label>
                <input name="id" autocomplete="off" type="text" id="user-id" />
            </li>
            <li>
                <label for="pwd">비밀번호</label>
                <input name="pwd" autocomplete="off" type="password" id="pwd" />
            </li>
        </ul>
    </fieldset>
    <input type="submit" value="로그인" />
</form>